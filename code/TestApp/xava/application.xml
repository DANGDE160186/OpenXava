<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="TestApp"> 		<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		  		<controller name="Typical"/>	</default-module>		<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadTeacher">		<model name="Teacher"/>		<controller name="Print"/>	</module>			-->    <module name="SkuVendorLeadTimeReport">        <model name="SkuVendorLeadTimeReport"/>        <controller name="ReportEx"/>        <mode-controller name="DetailOnly"/>     </module>    <module name="SkuVendorLeadTimeReportSql">        <model name="SkuVendorLeadTimeReportSql"/>        <controller name="ReportEx"/>        <mode-controller name="DetailOnly"/>     </module>    <module name="AdminDivTestCaseCountry">        <model name="AdminDivTestCase"/>        <view name="ShowCountryOnly"/>        <tab name="ShowCountryOnly"/>        <controller name="Typical"/>    </module>    <module name="SKUChangeLogVPD">	    <env-var name="XAVA_SEARCH_ACTION" value="CRUD.searchReadOnly"/>	    <model name="SKUChangeLog"/>	    <tab name="ShowCurrentUserOnly"/>	    <controller name="Print"/>    </module>    <module name="OnlyReadAccessTracking">        <model name="AccessTrackingEntity"/>        <controller name="Print"/>    </module>       	</application>